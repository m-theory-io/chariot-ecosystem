{
  "name": "chariot-codegen",
  "version": "0.1.0",
  "private": true,
  "description": "Visual DSL -> Chariot .ch code generator shared package",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,iife --global-name ChariotCodegen --dts --minify",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.4.0",
    "rimraf": "^5.0.5"
  }
}
